{
	"name": "karma-ui5",
	"description": "A Karma plugin for UI5",
	"version": "2.4.0",
	"author": "SAP SE (https://www.sap.com)",
	"license": "Apache-2.0",
	"homepage": "https://github.com/SAP/karma-ui5",
	"bugs": "https://github.com/SAP/karma-ui5/issues/",
	"keywords": [
		"ui5",
		"openui5",
		"sapui5",
		"karma-adapter",
		"karma-plugin"
	],
	"engines": {
		"node": ">= 10.0",
		"npm": ">= 5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/SAP/karma-ui5.git"
	},
	"main": "lib/index.js",
	"files": [
		"dist",
		"lib",
		"helper.js",
		"LICENSES/**",
		".reuse/**"
	],
	"scripts": {
		"lint": "eslint ./",
		"lint-fix": "eslint ./ --fix",
		"test": "npm run lint && npm run coverage && npm run integration",
		"unit": "jest -c test/unit/jest.config.js",
		"coverage": "jest -c test/unit/jest.config.js --coverage",
		"integration": "jest -c test/integration/jest.config.js",
		"integration-internet-explorer": "jest -c test/integration/jest.config.js --browsers=IE",
		"integration-saucelabs": "jest -c test/integration/jest.config.js --useSauceLabs=true",
		"dev": "webpack --watch --mode=development",
		"prepare": "webpack",
		"preversion": "npm test",
		"version": "git-chglog --next-tag v$npm_package_version -o CHANGELOG.md && git add CHANGELOG.md",
		"postversion": "git push --follow-tags",
		"release-note": "git-chglog -c .chglog/release-config.yml v$npm_package_version",
		"depcheck": "depcheck --ignores core-js,typescript,webpack-cli"
	},
	"peerDependencies": {
		"karma": ">= 4.3.0"
	},
	"dependencies": {
		"@ui5/fs": "^2.0.6",
		"@ui5/project": "^2.6.0",
		"@ui5/server": "^2.4.1",
		"express": "^4.18.2",
		"http-proxy": "^1.18.1",
		"js-yaml": "^4.1.0",
		"mkdirp": "^1.0.4"
	},
	"devDependencies": {
		"@babel/core": "^7.20.5",
		"@babel/preset-env": "^7.20.2",
		"@openui5/sap.ui.core": "^1.109.0",
		"babel-loader": "^8.3.0",
		"core-js": "^3.26.1",
		"depcheck": "^1.4.3",
		"eslint": "^7.32.0",
		"eslint-config-google": "^0.14.0",
		"eslint-plugin-jest": "^27.1.6",
		"eslint-plugin-jsdoc": "^37.9.7",
		"execa": "^5.1.1",
		"fast-glob": "^3.2.12",
		"istanbul-lib-coverage": "^3.2.0",
		"jest": "^27.5.1",
		"jest-extended": "^0.11.5",
		"karma": "^6.4.1",
		"karma-chrome-launcher": "^3.1.1",
		"karma-coverage": "2.0.3",
		"karma-ie-launcher": "^1.0.0",
		"karma-qunit": "^4.1.2",
		"karma-sauce-launcher": "^4.3.6",
		"license-webpack-plugin": "^4.0.2",
		"puppeteer": "^8.0.0",
		"qunit": "^2.19.3",
		"rimraf": "^3.0.2",
		"typescript": "^4.9.3",
		"webpack": "^5.75.0",
		"webpack-cli": "^4.10.0"
	}
}
